<div id="container">
    {{#each responseFromAPI}}
    <img src="{{this.image_url}}"/>
    <div id = "sub-div">
        {{> beerpartial }}
        <h3>Food Pairing</h3>
        <ul>
            {{#each food_pairing}}
                <li>{{this}}</li>
            {{/each}}
        </ul>
        <p><b>Brewer Tips: </b>{{this.brewers_tips}}</p>
    </div>
    {{/each}}
</div>